{
    "version": 2,
    "builds": [
        {
            "src": "api/chat.js",
            "use": "@vercel/node"
        },
        {
            "src": "Frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/chat",
            "dest": "/api/chat.js"
        },
        {
            "src": "/(.*)",
            "dest": "/Frontend/$1"
        }
    ]
}